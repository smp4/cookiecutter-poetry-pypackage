[flake8]
max-line-length = 100
exclude =
    .git,
    docs,
    node_modules
    __pycache__,
    build,
    *.pyc,
    *.egg-info,
    .cache,
    .eggs
max-complexity = 10
import-order-style = google
application-import-names = {{ cookiecutter.project_slug }}
